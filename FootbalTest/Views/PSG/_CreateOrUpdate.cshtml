@model FootballTeam.Models.PlayersPSG

@{
    ViewBag.Title = "PSG Players";
}

<h2>@ViewBag.Title</h2>
<div class="form-group" style="display:none;">
    @Html.LabelFor(m => m.PlayerID)
    @Html.EditorFor(m => m.PlayerID, new { htmlAttributes = new { @class = "form-control" } })
</div>
<div class="form-group">
    @Html.LabelFor(m => m.Name)
    @Html.EditorFor(m => m.Name, new { htmlAttributes = new { @class = "form-control" } })
</div>

<div class="form-group">
    @Html.LabelFor(m => m.Surname)
    @Html.EditorFor(m => m.Surname, new { htmlAttributes = new { @class = "form-control" } })
</div>

<div class="form-group">
    @Html.LabelFor(m => m.Position)
    @Html.EditorFor(m => m.Position, new { htmlAttributes = new { @class = "form-control" } })
</div>

<div class="form-group">
    @Html.LabelFor(m => m.Age)
    @Html.EditorFor(m => m.Age, new { htmlAttributes = new { @class = "form-control" } })
</div>


