
@{
    ViewBag.Title = "Create";
}


@using (Html.BeginForm("Create", "PSG", FormMethod.Post))
{
    <div class="container">
        <div class="row">
            <div class="col-9"></div>
            <div class="form-group col-3 text-right mt-3 mb-3 col-3">
                <input type="submit" value="Save" class="btn btn-primary" />
                <a href="@Url.Action("GetPlayersPSG", "PSG")" class="btn btn-default">Cancel</a>
            </div>
        </div>
    </div>


    @Html.Partial("_CreateOrUpdate");

}


